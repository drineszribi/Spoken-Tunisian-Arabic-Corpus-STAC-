        <tier id="TIE0" speaker="Sp1" type="t" category="Default" display-name="Sp1">
            <event start="T1" end="T2">آنَا عَنْدِي بَابَا وْخُويَا[lan:FR, déjà] بَطّْلُوا الدُخَّانْ خَاطِرْ بَابَا صَارِتْ لُو [lan:FR, crise cardiaque trois fois].</event>
            <event start="T2" end="T3">#</event>
            <event start="T3" end="T4">بَطَّلْ خُويَا ماللِي شَافْ بَابَا هَكَاكَة. وبَابَا بَطَّلْ تَوِيكَة. وآنَا مَعْنَاتْهَا مِنْ جُرِة الدُخَّانْ هَذَا [lan:FR, asmatique déjà].</event>
        </tier>
        <tier id="TIE1" speaker="Sp2" type="t" category="Default" display-name="Sp2">
            <event start="T4" end="T5">musique</event>
            <event start="T5" end="T6">آنَا عَنْدِي سَبْعَة سْنِينْ تَقْرِيبْ</event>
            <event start="T6" end="T7">نِتْكَيِفْ. حَاوِلْت</event>
            <event start="T7" end="T8">بَرْشَة مَرَّاتْ بَاشْ</event>
            <event start="T8" end="T9">نُقْلِعْ عْلَى</event>
            <event start="T9" end="T10">اِلْتَدْخِينْ.</event>
            <event start="T10" end="T11"> لكن مَا نِجِمْتِشْ.</event>
            <event start="T11" end="T12">عَنْدِي عَزِيمَة فِي كُلْ شَيْ.</event>
            <event start="T12" end="T13">أمَا عَزِيمْتِي خَانِتْنِي كَانْ فِي اِلْتَدْخِينْ.</event>
            <event start="T13" end="T14"> جَرِبْتْ [lan:FR, les patches].</event>
            <event start="T14" end="T15"> جَرِبْتْ حَتَّى [lan:FR, nicopas]. الحْرَابِشْ هَذُوكُمْ اللِي يْبِيعُوا فِيهُمْ فِي [lan:FR, pharmacie].</event>
            <event start="T15" end="T16">[lan:FR, dernièrement] خْذِيتْ هَاكَة [lan:FR,  cigarette électrique].</event>
            <event start="T16" end="T17">يَعْنِي تِشْعِلْ بِضَوْ ماهيش تْدَخِنْ.</event>
            <event start="T17" end="T18">تْخَرِجْ دُخَانْ [lan:MSA, فَقَطْ] لَكِنْ مَاهُوَاشْ</event>
            <event start="T18" end="T19">#</event>
            <event start="T19" end="T20">[lan:FR, sans nicotine voilà]. مَا نِجِمْتِشْ زَادَة.</event>
            <event start="T20" end="T21">ما نعرفش آنا.</event>
            <event start="T21" end="T22">#</event>
            <event start="T22" end="T23">عَنْدِي إقْتِرَاحْ.</event>
            <event start="T23" end="T24">نِقْتَرَحْ </event>
            <event start="T24" end="T25">اللِي </event>
            <event start="T25" end="T26">الدَوْلَة زَادَة تْخَمِمْ فِيهَا مْلِيحْ.</event>
            <event start="T26" end="T27">يَعْمْلُوا [lan:FR, des centres] مُوشْ لِيَّ آنَا بَرْكَة.</event>
            <event start="T27" end="T28"> يَعْنِي النَاسْ الكُلْ اللِي تْحِبْ </event>
            <event start="T28" end="T29">#</event>
            <event start="T29" end="T30">بِالرَسْمِي عَنْدِهُمْ [lan:FR, l'envie] بَاشْ يَقَلْعُوا عْلَى اِلْتَدْخِينْ.</event>
            <event start="T30" end="T31">(تنفس)</event>
            <event start="T31" end="T32">يَعْمْلُوا [lan:FR, des centres] مْتَاعْ يْعَاوْنُوا الَعْبَادْ بَاشْ تُقْلِعْ عْلَى اِلْتَدْخِينْ.</event>
        </tier>
        <tier id="TIE2" speaker="Sp3" type="t" category="Default" display-name="Sp3">
            <event start="T32" end="T33">musique</event>
            <event start="T33" end="T34">آنَا بَقِيِتْ ثْلَاثَة سْنِينْ مَا نِتْكَيِفِشْ.</event>
            <event start="T34" end="T35"> ورجَعِتْ لدُخَانْ.</event>
            <event start="T35" end="T36">#</event>
            <event start="T36" end="T37">ونْقُولْ لِجْمَاعَة اللِي يدَخْنُوا.</event>
            <event start="T37" end="T38">#</event>
            <event start="T38" end="T39">رُدُوا بَالْكُمْ تْبَطْلُوهْ. عْلَى خَاطِرْ رَاهُو يَبْقَى يْلُوم.ْ ويْقُولْ هَذِي العِشْرَة اللِي بِينَاتْنَا.</event>
        </tier>
        <tier id="TIE3" speaker="Sp4" type="t" category="Default" display-name="Sp4">
            <event start="T39" end="T40">musique</event>
            <event start="T40" end="T41">مَاهُوَاشْ مُسْتَحِيلْ الإِقْلَاعْ عَنْ اِلْتَدْخِينْ.</event>
            <event start="T41" end="T42">(تنفس)</event>
            <event start="T42" end="T43"> اللِي عَندْ عَزِيمَة يْنَجِمْ يْبَطِلْ الدُخَّان.</event>
            <event start="T43" end="T44">(تنفس)</event>
            <event start="T44" end="T45">يَا إما بِعَزِيمَتو وإلَا يَرَى حَدْ.</event>
            <event start="T45" end="T46">#</event>
            <event start="T46" end="T47">يَرَى حَدْ تْضَرْ مِنْ طَرَفْ مِنْ وْرَا الدُخَّانْ.</event>
            <event start="T47" end="T48">(تنفس)</event>
            <event start="T48" end="T49">مَثَلاً جاَتُو جَلْطَة. وَإلا وَلَّاتْ عَنْدُو شَرَايِينْ آ÷ عْرُوقْ مَسْدُودَة. فَهُوَ يِضطَرْ بَاشْ يْبَطِلْ الدُخَّانْ. عْلَاشْ يْقُولْ</event>
            <event start="T49" end="T50">#</event>
            <event start="T50" end="T51">مَا نَجْمَشْ. نَجِّمْ.</event>
            <event start="T51" end="T52">نَجِّمْ. نَجِّمْ. ونْزِيدْ نَجِّمْ.</event>
        </tier>
        <tier id="TIE4" speaker="Sp5" type="t" category="Default" display-name="Sp5">
            <event start="T52" end="T53">musique</event>
            <event start="T53" end="T54">آنَا قْعَدِتْ عَشْرَة سْنِينْ نِتْكَيِفْ.</event>
            <event start="T54" end="T55">#</event>
            <event start="T55" end="T56">وتَوَّة عَنْدِي سِتَة شْهُورْ ماللِي بَطَلِتْ.</event>
